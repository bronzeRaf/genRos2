<?xml version='1.0' encoding='UTF-8'?>
<metamodel:ROSSystem xmlns:xmi="http://www.omg.org/XMI" xmlns:metamodel="http://www.example.org/metamodel" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="My_dsl_ROS_system" xmi:version="2.0">
  <hasPackages xsi:type="metamodel:Package" name="interfaces" packagePath="">
    <hasTopicMessages xsi:type="metamodel:CustomMessage" name="Distance" description="This is a Message to provide a timestamped range value from Sonar sensors">
      <hasObjectProperties xsi:type="metamodel:ObjectProperty" name="h" description="Timestamp Header of the distance" default="">
        <datatype xsi:type="metamodel:ROSData" type="Header" package="std_msgs"/>
      </hasObjectProperties>
      <hasObjectProperties xsi:type="metamodel:ObjectProperty" name="range" description="range from obstacle in meters" default="">
        <datatype xsi:type="metamodel:Float"/>
      </hasObjectProperties>
    </hasTopicMessages>
  </hasPackages>
  <hasPackages xsi:type="metamodel:Package" name="geometry_msgs" packagePath="" builtin="true">
    <hasTopicMessages xsi:type="metamodel:RosMessage" name="Twist" package="geometry_msgs"/>
  </hasPackages>
  <hasPackages xsi:type="metamodel:Package" packagePath="path/to/pack1" maintainer="bronzeRaf" name="obstacle_avoidance" license="Apache License 2.0" email="rnm1816@gmail.com" description="This is a package that implements an obstacle avoidance algorithm for a robot of one CPU and two SONARS.">
    <hasNodes xsi:type="metamodel:Node" name="sonar1" namespace="">
      <hasPublishers xsi:type="metamodel:Publisher" name="toCpu1" topicPath="topic/path/s1" pmsg="//@hasPackages.0/@hasTopicMessages.0" publishRate="0.5" qosprofile="//@hasQosProfiles.0"/>
    </hasNodes>
    <hasNodes xsi:type="metamodel:Node" name="sonar2" namespace="">
      <hasPublishers xsi:type="metamodel:Publisher" name="toCpu2" topicPath="topic/path/s2" pmsg="//@hasPackages.0/@hasTopicMessages.0" publishRate="0.5" qosprofile="//@hasQosProfiles.0"/>
    </hasNodes>
    <hasNodes xsi:type="metamodel:Node" name="cpu" namespace="">
      <hasSubscribers xsi:type="metamodel:Subscriber" topicPath="topic/path/s1" name="fromSonar1" smsg="//@hasPackages.0/@hasTopicMessages.0" qosprofile="//@hasQosProfiles.0"/>
      <hasSubscribers xsi:type="metamodel:Subscriber" topicPath="topic/path/s2" name="fromSonar2" smsg="//@hasPackages.0/@hasTopicMessages.0" qosprofile="//@hasQosProfiles.0"/>
      <hasPublishers xsi:type="metamodel:Publisher" name="toActuators" topicPath="topic/path/act" publishRate="0.5" pmsg="//@hasPackages.1/@hasTopicMessages.0"/>
    </hasNodes>
    <hasDocumentation xsi:type="metamodel:Documentation"/>
  </hasPackages>
  <graph xsi:type="metamodel:Graph" nodes="//@hasPackages.2/@hasNodes.0 //@hasPackages.2/@hasNodes.1 //@hasPackages.2/@hasNodes.2">
    <hasTopics xsi:type="metamodel:Topic" topicPath="topic/path/s1" subscriber="//@hasPackages.2/@hasNodes.2/@hasSubscribers.0" publisher="//@hasPackages.2/@hasNodes.0/@hasPublishers.0"/>
    <hasTopics xsi:type="metamodel:Topic" topicPath="topic/path/s2" subscriber="//@hasPackages.2/@hasNodes.2/@hasSubscribers.1" publisher="//@hasPackages.2/@hasNodes.1/@hasPublishers.0"/>
    <hasTopics xsi:type="metamodel:Topic" topicPath="topic/path/act" publisher="//@hasPackages.2/@hasNodes.2/@hasPublishers.0"/>
  </graph>
  <hasQosProfiles xsi:type="metamodel:CustomQosProfile" history="KEEP_LAST" depth="15"/>
</metamodel:ROSSystem>
