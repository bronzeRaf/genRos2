<?xml version='1.0' encoding='UTF-8'?>
<metamodel:ROSSystem xmlns:xmi="http://www.omg.org/XMI" xmlns:metamodel="http://www.example.org/metamodel" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="My_first_ROS_system2" xmi:version="2.0">
  <hasPackages xsi:type="metamodel:Package" packagePath="path to pack1" name="pack1">
    <hasDocumentation xsi:type="metamodel:Documentation"/>
    <hasRosServices xsi:type="metamodel:RosService" package="std_srvs" name="SetBool"/>
    <hasNodes xsi:type="metamodel:Node" name="node1">
      <hasParameters xsi:type="metamodel:Parameter" name="p1" value="32"/>
      <hasPublishers xsi:type="metamodel:Publisher" pmsg="//@hasPackages.0/@hasRosMessages.0" name="publy3" topicPath="topic/path3" publishRate="12.0"/>
      <hasPublishers xsi:type="metamodel:Publisher" name="publy2" topicPath="topic/path2" pmsg="//@hasCustomMessages.0" publishRate="8.0" qosprofile="//@hasCustomQosProfiles.0"/>
      <hasServers xsi:type="metamodel:Server" servicePath="ser/vice/path1" serviceName="add_two" name="Server1" servicemessage="//@hasCustomServices.0"/>
      <hasServers xsi:type="metamodel:Server" servicePath="ser/vice/path3" serviceName="set_bool" name="Server3" servicemessage="//@hasPackages.0/@hasRosServices.0"/>
      <hasActionServers xsi:type="metamodel:ActionServer" actioninterface="//@hasCustomActionInterfaces.0" name="action1"/>
    </hasNodes>
    <hasNodes xsi:type="metamodel:Node" name="node2">
      <hasSubscribers xsi:type="metamodel:Subscriber" topicPath="topic/path2" smsg="//@hasCustomMessages.0" qosprofile="//@hasCustomQosProfiles.0" name="suby2"/>
      <hasActionClients xsi:type="metamodel:ActionClient" name="action1" actioninterface="//@hasCustomActionInterfaces.0"/>
      <hasClients xsi:type="metamodel:Client" servicemessage="//@hasCustomServices.0" name="Client1" servicePath="ser/vice/path1" serviceName="add_two"/>
      <hasClients xsi:type="metamodel:Client" servicemessage="//@hasPackages.0/@hasRosServices.0" name="Client3" servicePath="ser/vice/path3" serviceName="set_bool"/>
    </hasNodes>
    <hasRosMessages xsi:type="metamodel:RosMessage" name="Header" package="std_msgs"/>
  </hasPackages>
  <hasPackages xsi:type="metamodel:Package" packagePath="path to pack2" name="pack2">
    <hasDocumentation xsi:type="metamodel:Documentation"/>
    <hasNodes xsi:type="metamodel:Node" name="node3">
      <hasSubscribers xsi:type="metamodel:Subscriber" topicPath="topic/path3" smsg="//@hasPackages.0/@hasRosMessages.0" name="suby3"/>
      <hasClients xsi:type="metamodel:Client" servicemessage="//@hasCustomServices.1" name="Client2" servicePath="ser/vice/path2" serviceName="str"/>
    </hasNodes>
  </hasPackages>
  <hasCustomMessages xsi:type="metamodel:CustomMessage" description="Message for passing a header and an integer" name="ValueInt">
    <hasObjectProperties xsi:type="metamodel:ObjectProperty" name="header" description="header type object">
      <datatype xsi:type="metamodel:ROSData" package="std_msgs" type="Header"/>
    </hasObjectProperties>
    <hasObjectProperties xsi:type="metamodel:ObjectProperty" name="x" description="x value for integer message">
      <datatype xsi:type="metamodel:Int" type="int32"/>
    </hasObjectProperties>
  </hasCustomMessages>
  <hasCustomMessages xsi:type="metamodel:CustomMessage" description="Message for testing purposes" name="ValueString">
    <hasObjectProperties xsi:type="metamodel:ObjectProperty" name="x" description="x value for string message">
      <datatype xsi:type="metamodel:String" type="string"/>
    </hasObjectProperties>
  </hasCustomMessages>
  <hasCustomActionInterfaces xsi:type="metamodel:CustomActionInterface" name="Increase" description="Action for enabling counters">
    <hasResult xsi:type="metamodel:Result">
      <hasObjectProperties xsi:type="metamodel:ObjectProperty" name="a" description="a value for result trigger">
        <datatype xsi:type="metamodel:Int" type="int64"/>
      </hasObjectProperties>
    </hasResult>
    <hasGoal xsi:type="metamodel:Goal">
      <hasObjectProperties xsi:type="metamodel:ObjectProperty" name="start" description="a value for start value">
        <datatype xsi:type="metamodel:Int" type="int64"/>
      </hasObjectProperties>
      <hasObjectProperties xsi:type="metamodel:ObjectProperty" name="goal" description="a value for goal value">
        <datatype xsi:type="metamodel:Int" type="int64"/>
      </hasObjectProperties>
    </hasGoal>
    <hasFeedback xsi:type="metamodel:Feedback">
      <hasObjectProperties xsi:type="metamodel:ObjectProperty" name="update" description="a value for feedback">
        <datatype xsi:type="metamodel:Int" type="int64"/>
      </hasObjectProperties>
    </hasFeedback>
  </hasCustomActionInterfaces>
  <topology xsi:type="metamodel:Topology"/>
  <hasGraphs xsi:type="metamodel:Graph" nodes="//@hasPackages.0/@hasNodes.0 //@hasPackages.0/@hasNodes.1 //@hasPackages.1/@hasNodes.0"/>
  <hasCustomServices xsi:type="metamodel:CustomService" description="Service for receiving two numbers and returning their their sum" name="Addtwo">
    <hasRequest xsi:type="metamodel:Request">
      <hasObjectProperties xsi:type="metamodel:ObjectProperty" name="a" description="a value for integer service Addtwo request 1">
        <datatype xsi:type="metamodel:ROSData" package="std_msgs" type="Int32"/>
      </hasObjectProperties>
      <hasObjectProperties xsi:type="metamodel:ObjectProperty" name="b" description="b value for integer service Addtwo request 2">
        <datatype xsi:type="metamodel:Int" type="int64"/>
      </hasObjectProperties>
    </hasRequest>
    <hasResponse xsi:type="metamodel:Response">
      <hasObjectProperties xsi:type="metamodel:ObjectProperty" name="c" description="c value for integer service Addtwo response">
        <datatype xsi:type="metamodel:Int" type="int32"/>
      </hasObjectProperties>
    </hasResponse>
  </hasCustomServices>
  <hasCustomServices xsi:type="metamodel:CustomService" description="Service for testing purposes" name="SrFloatFloatString">
    <hasRequest xsi:type="metamodel:Request">
      <hasObjectProperties xsi:type="metamodel:ObjectProperty" name="x" description="x value for float service SrFloatFloatString request 1">
        <datatype xsi:type="metamodel:Float" type="float64"/>
      </hasObjectProperties>
      <hasObjectProperties xsi:type="metamodel:ObjectProperty" name="y" description="y value for float service SrFloatFloatString request 2">
        <datatype xsi:type="metamodel:Float"/>
      </hasObjectProperties>
    </hasRequest>
    <hasResponse xsi:type="metamodel:Response">
      <hasObjectProperties xsi:type="metamodel:ObjectProperty" name="z" description="z value for string service SrFloatFloatString response">
        <datatype xsi:type="metamodel:String" type="string"/>
      </hasObjectProperties>
    </hasResponse>
  </hasCustomServices>
  <hasCustomQosProfiles xsi:type="metamodel:CustomQosProfile" depth="10" history="KEEP_ALL" reliability="RELIABLE" durability="TRANSIENT_LOCAL"/>
</metamodel:ROSSystem>
