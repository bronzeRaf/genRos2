<?xml version='1.0' encoding='UTF-8'?>
<metamodel:ROSSystem xmlns:xmi="http://www.omg.org/XMI" xmlns:metamodel="http://www.example.org/metamodel" name="My_first_ROS_system" xmi:version="2.0">
  <topology/>
  <hasGraphs nodes="//@hasPackages.0/@hasNodes.0 //@hasPackages.0/@hasNodes.1 //@hasPackages.1/@hasNodes.0"/>
  <hasPackages packagePath="path to pack1" name="pack1">
    <hasNodes name="Nodes_name">
      <hasPublishers publishRate="0.5" pmsg="//@hasTopicMessages.0" topicPath="topic/path" name="publy1"/>
      <hasPublishers publishRate="0.8" pmsg="//@hasTopicMessages.1" topicPath="topic/path2" name="publy3"/>
      <hasServers name="Server1" servicemessage="//@hasServiceMessages.0" servicePath="ser/vice/path"/>
    </hasNodes>
    <hasNodes name="Node_2">
      <hasSubscribers topicPath="topic/path2" name="suby2" smsg="//@hasTopicMessages.1"/>
      <hasClients servicemessage="//@hasServiceMessages.0" name="Client1" servicePath="ser/vice/path"/>
    </hasNodes>
    <hasDocumentation/>
  </hasPackages>
  <hasPackages packagePath="path to pack2" name="pack2">
    <hasNodes name="Node_3">
      <hasSubscribers topicPath="topic/path" name="suby1" smsg="//@hasTopicMessages.0"/>
    </hasNodes>
    <hasDocumentation/>
  </hasPackages>
  <hasServiceMessages name="Addtwo">
    <hasResponse>
      <hasCommunicationObjects name="c" type="int32"/>
    </hasResponse>
    <hasRequest>
      <hasCommunicationObjects name="a" type="int32"/>
      <hasCommunicationObjects name="b" type="int32"/>
    </hasRequest>
  </hasServiceMessages>
  <hasServiceMessages name="SrFloatFloatString">
    <hasResponse>
      <hasCommunicationObjects name="z" type="string"/>
    </hasResponse>
    <hasRequest>
      <hasCommunicationObjects name="x"/>
      <hasCommunicationObjects name="y"/>
    </hasRequest>
  </hasServiceMessages>
  <hasTopicMessages name="ValueInt">
    <hasCommunicationObjects name="x" type="int32"/>
  </hasTopicMessages>
  <hasTopicMessages name="ValueString">
    <hasCommunicationObjects name="x" type="string"/>
  </hasTopicMessages>
</metamodel:ROSSystem>
