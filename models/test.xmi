<?xml version='1.0' encoding='UTF-8'?>
<metamodel:ROSSystem xmlns:xmi="http://www.omg.org/XMI" xmlns:metamodel="http://www.example.org/metamodel" name="My_first_ROS_system" xmi:version="2.0">
  <topology/>
  <hasGraphs nodes="//@hasPackages.0/@hasNodes.0 //@hasPackages.0/@hasNodes.1 //@hasPackages.0/@hasNodes.2"/>
  <hasPackages packagePath="path to pack1" name="pack1">
    <hasDocumentation/>
    <hasNodes name="Nodes_name">
      <hasPublishers topicPath="topic/path" name="publy1" publishRate="0.5" pmsg="//@hasTopicMessages.0"/>
      <hasPublishers topicPath="topic/path2" name="publy3" publishRate="0.8" pmsg="//@hasTopicMessages.1"/>
      <hasServers servicePath="ser/vice/path" name="Server1" servicemessage="//@hasServiceMessages.0"/>
    </hasNodes>
    <hasNodes name="Node_2">
      <hasClients servicePath="ser/vice/path" servicemessage="//@hasServiceMessages.0" name="Client1"/>
      <hasSubscribers name="suby1" smsg="//@hasTopicMessages.0" topicPath="topic/path"/>
      <hasSubscribers name="suby2" smsg="//@hasTopicMessages.1" topicPath="topic/path2"/>
    </hasNodes>
    <hasNodes name="Node_3"/>
  </hasPackages>
  <hasServiceMessages name="Addtwo">
    <hasRequest>
      <hasCommunicationObjects type="int32" name="a"/>
      <hasCommunicationObjects type="int32" name="b"/>
    </hasRequest>
    <hasResponse>
      <hasCommunicationObjects type="int32" name="c"/>
    </hasResponse>
  </hasServiceMessages>
  <hasServiceMessages name="SrFloatFloatString">
    <hasRequest>
      <hasCommunicationObjects name="x"/>
      <hasCommunicationObjects name="y"/>
    </hasRequest>
    <hasResponse>
      <hasCommunicationObjects type="string" name="z"/>
    </hasResponse>
  </hasServiceMessages>
  <hasTopicMessages name="ValueInt">
    <hasCommunicationObjects type="int32" name="x"/>
  </hasTopicMessages>
  <hasTopicMessages name="ValueString">
    <hasCommunicationObjects type="string" name="x"/>
  </hasTopicMessages>
</metamodel:ROSSystem>
