<?xml version='1.0' encoding='UTF-8'?>
<metamodel:ROSSystem xmlns:xmi="http://www.omg.org/XMI" xmlns:metamodel="http://www.example.org/metamodel" name="My_first_ROS_system" xmi:version="2.0">
  <hasTopicMessages name="ValueInt">
    <hasCommunicationObjects name="x" type="int32"/>
  </hasTopicMessages>
  <hasTopicMessages name="ValueString">
    <hasCommunicationObjects name="x" type="string"/>
  </hasTopicMessages>
  <hasGraphs nodes="//@hasPackages.0/@hasNodes.0 //@hasPackages.0/@hasNodes.1 //@hasPackages.1/@hasNodes.0"/>
  <topology/>
  <hasPackages name="pack1" packagePath="path to pack1">
    <hasNodes name="Nodes_name">
      <hasServers serviceName="add_two" name="Server1" servicePath="ser/vice/path" servicemessage="//@hasServiceMessages.0"/>
      <hasPublishers name="publy1" pmsg="//@hasTopicMessages.0" publishRate="0.5" topicPath="topic/path"/>
      <hasPublishers name="publy3" pmsg="//@hasTopicMessages.1" publishRate="0.8" topicPath="topic/path2"/>
    </hasNodes>
    <hasNodes name="Node_2">
      <hasClients name="Client1" serviceName="add_two" servicePath="ser/vice/path" servicemessage="//@hasServiceMessages.0"/>
      <hasSubscribers smsg="//@hasTopicMessages.1" name="suby2" topicPath="topic/path2"/>
    </hasNodes>
    <hasDocumentation/>
  </hasPackages>
  <hasPackages name="pack2" packagePath="path to pack2">
    <hasNodes name="Node_3">
      <hasSubscribers smsg="//@hasTopicMessages.0" name="suby1" topicPath="topic/path"/>
    </hasNodes>
    <hasDocumentation/>
  </hasPackages>
  <hasServiceMessages name="Addtwo">
    <hasRequest>
      <hasCommunicationObjects name="a" type="int32"/>
      <hasCommunicationObjects name="b" type="int32"/>
    </hasRequest>
    <hasResponse>
      <hasCommunicationObjects name="c" type="int32"/>
    </hasResponse>
  </hasServiceMessages>
  <hasServiceMessages name="SrFloatFloatString">
    <hasRequest>
      <hasCommunicationObjects name="x"/>
      <hasCommunicationObjects name="y"/>
    </hasRequest>
    <hasResponse>
      <hasCommunicationObjects name="z" type="string"/>
    </hasResponse>
  </hasServiceMessages>
</metamodel:ROSSystem>
